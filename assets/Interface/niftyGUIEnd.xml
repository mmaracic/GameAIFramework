<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="http://nifty-gui.sourceforge.net/nifty-1.3.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nifty-gui.sourceforge.net/nifty-1.3.xsd http://nifty-gui.sourceforge.net/nifty-1.3.xsd">
  <!-- +++++++++++++++++++++++++++++++++++++++ -->
  <!-- start screen -->
  <!-- +++++++++++++++++++++++++++++++++++++++ -->
  <screen id="end" controller="mmaracic.monkeyengine.GUIAppState">
    <layer id="layer" backgroundColor="#000f" childLayout="center">
        <panel id="left" height="100%" width="25%"  align="center" halign="center" valign="center" childLayout="center">
        </panel>        
        <panel id="center" height="100%" width="50%" align="center" valign="center" backgroundColor="#000f" childLayout="vertical" visibleToMouse="true">
            <panel height="25%" width="100%" align="center" valign="center" childLayout="center" visibleToMouse="true">
                <control name="label" text="Outcome" id="labOutcome" align="center" valign="center"/>
            </panel>
            <panel height="25%" width="100%" align="center" valign="center" childLayout="center" visibleToMouse="true">
                <!--<control name="label" text="Soft outcome" id="labSoftOutcome" align="center" valign="center"/>-->
                <text text="Soft outcome" id="labSoftOutcome" wrap="true" style="nifty-label" align="center" valign="center"/>
            </panel>
            <panel height="50%" width="100%" align="center" valign="center" childLayout="center" visibleToMouse="true">
                <control name="button" label="Go to Main Menu" id="cmdReset" align="center" valign="center" visibleToMouse="true">
                    <interact onClick="mainMenu()"/>
                </control>
            </panel>
        </panel>
        <panel id="right" height="100%" width="25%"  align="center" halign="center" valign="center" childLayout="center">
        </panel>
    </layer>
  </screen>
</nifty>
